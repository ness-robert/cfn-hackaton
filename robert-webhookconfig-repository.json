{
    "typeName": "Robert::WebhookConfig::Repository",
    "description": "An example resource schema demonstrating some basic constructs and validation rules.",
    "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-rpdk.git",
    "definitions": { },
    "properties": {
        "Id": {
            "description": "The id of the resource.",
            "type": "string",
            "minLength": 1
        },
        "ServiceUsername": {
            "description": "Bitbucket service username.",
            "type": "string",
            "minLength": 1
        },
        "ServiceAppPassword": {
            "description": "Bitbucket app password.",
            "type": "string",
            "minLength": 1
        },
        "Workspace": {
            "description": "Bitbucket workspace slug in which the repository is located.",
            "type": "string",
            "minLength": 1
        },
        "Repository": {
            "description": "Repository slug for which the webhook needs to be configured.",
            "type": "string",
            "minLength": 1
        },
        "WebhookUrl": {
            "description": "The url to call when an event is triggered.",
            "type": "string",
            "pattern": "^https?://[^\\s/$.?#].[^\\s]*$"
        }
    },
    "additionalProperties": false,
    "required": [
        "ServiceUsername",
        "ServiceAppPassword",
        "Workspace",
        "Repository",
        "WebhookUrl"
    ],
    "createOnlyProperties": [
        "/properties/Id"
    ],
    "readOnlyProperties": [
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "initech:CreateReport"
            ]
        },
        "read": {
            "permissions": [
                "initech:DescribeReport"
            ]
        },
        "update": {
            "permissions": [
                "initech:UpdateReport"
            ]
        },
        "delete": {
            "permissions": [
                "initech:DeleteReport"
            ]
        },
        "list": {
            "permissions": [
                "initech:ListReports"
            ]
        }
    }
}
